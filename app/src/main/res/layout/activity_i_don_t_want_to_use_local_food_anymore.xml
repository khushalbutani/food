<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="iDonTWantToUseLocalFoodAnymoreVM" type="com.khushalsapplication.app.modules.idontwanttouselocalfoodanymore.data.viewmodel.IDonTWantToUseLocalFoodAnymoreVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumnrectangleninetyfour"
        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/_663pxv"
        android:layout_marginTop="@dimen/_9pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_13pxh"
            android:layout_marginStart="@dimen/_13pxh"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linearRowrectangleninetyfour"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageRectangleNinetyFour"
                    android:layout_height="@dimen/_34pxh"
                    android:layout_width="@dimen/_34pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_rectangle94"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle94"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtNext"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:layout_marginStart="@dimen/_327pxh"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_weight="0.52"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{iDonTWantToUseLocalFoodAnymoreVM.iDonTWantToUseLocalFoodAnymoreModel.txtNext}"
                    style="@style/txtDMSansregular18_2" tools:text="@string/lbl_next"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:id="@+id/linearIdontwantto"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_11pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700">
            <TextView android:gravity="start"
                android:id="@+id/txtIdontwantto"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_19pxh"
                android:layout_marginStart="@dimen/_19pxh"
                android:layout_width="match_parent"
                android:text="@{iDonTWantToUseLocalFoodAnymoreVM.iDonTWantToUseLocalFoodAnymoreModel.txtIdontwantto}"
                style="@style/txtDMSansmedium27" tools:text="@string/msg_i_don_t_want_to2"/>
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearColumndescription"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_3pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylewhite_6">
                <TextView android:gravity="start"
                    android:id="@+id/txtDescription"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginEnd="@dimen/_13pxh"
                    android:layout_marginStart="@dimen/_13pxh"
                    android:layout_marginTop="@dimen/_26pxv"
                    android:layout_width="match_parent"
                    android:text="@{iDonTWantToUseLocalFoodAnymoreVM.iDonTWantToUseLocalFoodAnymoreModel.txtDescription}"
                    style="@style/txtDMSansregular18_3" tools:text="@string/msg_do_you_have_any"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
