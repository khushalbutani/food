<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="orderVM" type="com.khushalsapplication.app.modules.order.data.viewmodel.OrderVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumnellipsetwentysix"
        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/_74pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true" android:gravity="start"
            android:id="@+id/scrollViewScrollview"
            android:layout_height="0dp" android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:gravity="center_horizontal"
                android:id="@+id/linearOrder"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <FrameLayout android:gravity="start"
                    android:id="@+id/frameStacksubtotal"
                    android:layout_height="@dimen/_318pxv" android:layout_width="match_parent">
                    <FrameLayout
                        android:id="@+id/frameStackellipsetwentysix"
                        android:layout_gravity="top|center"
                        android:layout_height="@dimen/_222pxv"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginStart="@dimen/_10pxh" android:layout_width="@dimen/_414pxh">
                        <ImageView
                            android:id="@+id/imageEllipseTwentySix"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_222pxh"
                            android:layout_width="match_parent"
                            android:scaleType="fitXY"
                            imageRes="@{@drawable/img_ellipse26}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse26"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtClose"
                            android:layout_gravity="top|end"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_61pxv"
                            android:layout_marginEnd="@dimen/_23pxh"
                            android:layout_marginStart="@dimen/_23pxh"
                            android:layout_marginTop="@dimen/_61pxv"
                            android:layout_width="wrap_content"
                            android:singleLine="true"
                            android:text="@{orderVM.orderModel.txtClose}"
                            style="@style/txtDMSansbold22" tools:text="@string/lbl_close"/>
                    </FrameLayout>
                    <FrameLayout
                        android:id="@+id/frameStackellipsetwentyseven"
                        android:layout_gravity="top|start"
                        android:layout_height="@dimen/_194pxv"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_10pxh" android:layout_width="@dimen/_284pxh">
                        <ImageView
                            android:id="@+id/imageEllipseTwentySeven"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_194pxh"
                            android:layout_width="match_parent"
                            android:scaleType="fitXY"
                            imageRes="@{@drawable/img_ellipse27}"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse27"/>
                        <LinearLayout
                            android:id="@+id/linearColumnrectangleninetyfour"
                            android:layout_gravity="top|start"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_10pxv"
                            android:layout_marginEnd="@dimen/_11pxh"
                            android:layout_marginStart="@dimen/_11pxh"
                            android:layout_marginTop="@dimen/_9pxv"
                            android:layout_width="wrap_content" android:orientation="vertical">
                            <ImageView
                                android:id="@+id/imageRectangleNinetyFour"
                                android:layout_gravity="start"
                                android:layout_height="@dimen/_34pxh"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:layout_width="@dimen/_34pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_rectangle94"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle94"/>
                            <TextView android:ellipsize="end"
                                android:gravity="end"
                                android:id="@+id/txtOrder"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_10pxh"
                                android:layout_marginTop="@dimen/_5pxv"
                                android:layout_width="match_parent"
                                android:singleLine="true"
                                android:text="@{orderVM.orderModel.txtOrder}"
                                style="@style/txtDMSansbold39" tools:text="@string/lbl_order"/>
                        </LinearLayout>
                    </FrameLayout>
                    <LinearLayout android:id="@+id/linearColumnsubtotal"
                        android:layout_gravity="bottom|center"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_8pxh"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent"
                        android:orientation="vertical" style="@style/groupStylewhite_11">
                        <LinearLayout android:gravity="start"
                            android:id="@+id/linearRowsubtotal"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_16pxh"
                            android:layout_marginStart="@dimen/_16pxh"
                            android:layout_marginTop="@dimen/_22pxv"
                            android:layout_width="match_parent" android:orientation="horizontal">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtSubtotal"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_weight="0.66"
                                android:layout_width="0dp"
                                android:singleLine="true"
                                android:text="@{orderVM.orderModel.txtSubtotal}"
                                style="@style/txtDMSansmedium21" tools:text="@string/lbl_subtotal"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtFourHundred"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_215pxh"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_weight="0.34"
                                android:layout_width="0dp"
                                android:singleLine="true"
                                android:text="@{orderVM.orderModel.txtFourHundred}"
                                style="@style/txtDMSansmedium21_2" tools:text="@string/lbl_400"/>
                        </LinearLayout>
                        <LinearLayout android:gravity="start"
                            android:id="@+id/linearRowtaxfees"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_16pxh"
                            android:layout_marginStart="@dimen/_16pxh"
                            android:layout_marginTop="@dimen/_8pxv"
                            android:layout_width="match_parent" android:orientation="horizontal">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtTaxFees"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_weight="0.7"
                                android:layout_width="0dp"
                                android:singleLine="true"
                                android:text="@{orderVM.orderModel.txtTaxFees}"
                                style="@style/txtDMSansmedium21" tools:text="@string/lbl_tax_fees"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtFree"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_198pxh"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_weight="0.3"
                                android:layout_width="0dp"
                                android:singleLine="true"
                                android:text="@{orderVM.orderModel.txtFree}"
                                style="@style/txtDMSansmedium21_2" tools:text="@string/lbl_free"/>
                        </LinearLayout>
                        <LinearLayout android:gravity="start"
                            android:id="@+id/linearRowdelivery"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_87pxv"
                            android:layout_marginEnd="@dimen/_16pxh"
                            android:layout_marginStart="@dimen/_16pxh"
                            android:layout_marginTop="@dimen/_8pxv"
                            android:layout_width="match_parent" android:orientation="horizontal">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtDelivery"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_weight="0.65"
                                android:layout_width="0dp"
                                android:singleLine="true"
                                android:text="@{orderVM.orderModel.txtDelivery}"
                                style="@style/txtDMSansmedium21" tools:text="@string/lbl_delivery"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtFreeOne"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginStart="@dimen/_218pxh"
                                android:layout_weight="0.35"
                                android:layout_width="0dp"
                                android:singleLine="true"
                                android:text="@{orderVM.orderModel.txtFreeOne}"
                                style="@style/txtDMSansmedium21_2" tools:text="@string/lbl_free"/>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearRowlanguage"
                        android:layout_gravity="bottom|center"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_6pxh"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="match_parent"
                        android:orientation="horizontal" style="@style/groupStylewhite_12">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtLanguage"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_13pxv"
                            android:layout_marginStart="@dimen/_18pxh"
                            android:layout_marginTop="@dimen/_5pxv"
                            android:layout_weight="0.4"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{orderVM.orderModel.txtLanguage}"
                            style="@style/txtDMSansbold37" tools:text="@string/lbl_total"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txt42646rs"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_15pxv"
                            android:layout_marginEnd="@dimen/_25pxh"
                            android:layout_marginStart="@dimen/_146pxh"
                            android:layout_marginTop="@dimen/_11pxv"
                            android:layout_weight="0.6"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{orderVM.orderModel.txt42646rs}"
                            style="@style/txtDMSansbold31_1" tools:text="@string/lbl_426_46rs"/>
                    </LinearLayout>
                </FrameLayout>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnCheckout"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_34pxh"
                    android:layout_marginStart="@dimen/_34pxh"
                    android:layout_marginTop="@dimen/_448pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_9pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_9pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_checkout"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded_13" tools:text="@string/lbl_checkout"/>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</layout>
