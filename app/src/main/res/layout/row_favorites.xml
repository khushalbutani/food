<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="favoritesRowModel" type="com.khushalsapplication.app.modules.favorites.data.model.FavoritesRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearRowrectangleeightyeight"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" android:orientation="horizontal">
        <FrameLayout android:id="@+id/frameStackrectangleeightyeight"
            android:layout_height="@dimen/_279pxv" android:layout_width="@dimen/_215pxh">
            <LinearLayout
                android:id="@+id/linearColumnrectangleeightyeight"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_2pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylewhite_2">
                <ImageView android:id="@+id/imageRectangleEightyEight"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_197pxh"
                    android:layout_marginEnd="@dimen/_8pxh"
                    android:layout_marginStart="@dimen/_15pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="@dimen/_192pxh"
                    android:scaleType="fitXY"
                    cornerRadius="@{@dimen/_15pxh}"
                    imageRes="@{@drawable/img_rectangle88_197x192}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle88_197x192"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLanguage"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_36pxv"
                    android:layout_marginEnd="@dimen/_15pxh"
                    android:layout_marginStart="@dimen/_15pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{favoritesRowModel.txtLanguage}"
                    style="@style/txtDMSansmedium16_1" tools:text="@string/lbl_japanese"/>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtSushiToday"
                android:layout_gravity="bottom|start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{favoritesRowModel.txtSushiToday}"
                style="@style/txtDMSansmedium16_1" tools:text="@string/lbl_sushi_today"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtNigirisushi"
                android:layout_gravity="bottom|start"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_20pxv"
                android:layout_marginEnd="@dimen/_15pxh"
                android:layout_marginStart="@dimen/_15pxh"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{favoritesRowModel.txtNigirisushi}"
                style="@style/txtDMSansbold17" tools:text="@string/lbl_nigiri_sushi"/>
        </FrameLayout>
        <FrameLayout android:id="@+id/frameStacklanguageOne"
            android:layout_height="@dimen/_277pxv"
            android:layout_marginBottom="@dimen/_1pxv"
            android:layout_marginStart="@dimen/_5pxh" android:layout_width="@dimen/_215pxh">
            <TextView android:ellipsize="end"
                android:id="@+id/txtLanguageOne"
                android:layout_gravity="bottom|start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_9pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{favoritesRowModel.txtLanguageOne}"
                style="@style/txtDMSansmedium16_1" tools:text="@string/lbl_shurbhi"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtPavBhaji"
                android:layout_gravity="bottom|start"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_19pxv"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_8pxh"
                android:layout_marginTop="@dimen/_19pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{favoritesRowModel.txtPavBhaji}"
                style="@style/txtDMSansbold17" tools:text="@string/lbl_pav_bhaji"/>
            <LinearLayout
                android:id="@+id/linearColumnrectangleeightynine"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylewhite_2">
                <ImageView android:id="@+id/imageRectangleEightyNine"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_197pxh"
                    android:layout_marginEnd="@dimen/_15pxh"
                    android:layout_marginStart="@dimen/_8pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_width="@dimen/_192pxh"
                    android:scaleType="fitXY"
                    cornerRadius="@{@dimen/_15pxh}"
                    imageRes="@{@drawable/img_rectangle89_197x192}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle89_197x192"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtIndian"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_38pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_8pxh"
                    android:layout_marginTop="@dimen/_11pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{favoritesRowModel.txtIndian}"
                    style="@style/txtDMSansmedium16_1" tools:text="@string/lbl_indian"/>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
