<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="cuisinesRowModel" type="com.khushalsapplication.app.modules.cuisines.data.model.CuisinesRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearRowrectangle117"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="horizontal" style="@style/groupStylewhite_2">
        <LinearLayout android:id="@+id/linearRowrectangle106"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_7pxv"
            android:layout_marginStart="@dimen/_27pxh"
            android:layout_marginTop="@dimen/_4pxv"
            android:layout_weight="0.91" android:layout_width="0dp" android:orientation="horizontal">
            <ImageView android:id="@+id/imageRectangle106"
                android:layout_height="@dimen/_45pxh"
                android:layout_width="@dimen/_51pxh"
                android:scaleType="fitXY" cornerRadius="@{@dimen/_5pxh}"
                imageRes="@{@drawable/img_rectangle106}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle106"/>
            <FrameLayout android:id="@+id/frameStacklanguage"
                android:layout_height="@dimen/_39pxv"
                android:layout_marginBottom="@dimen/_2pxv"
                android:layout_marginStart="@dimen/_18pxh"
                android:layout_marginTop="@dimen/_2pxv" android:layout_width="@dimen/_97pxh">
                <TextView android:ellipsize="end"
                    android:gravity="top|start"
                    android:id="@+id/txtLanguageOne"
                    android:layout_gravity="top|center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{cuisinesRowModel.txtLanguageOne}"
                    style="@style/txtDMSansmedium21" tools:text="@string/lbl_japanese"/>
                <TextView android:ellipsize="end"
                    android:gravity="top|start"
                    android:id="@+id/txtRestaurantsCounter"
                    android:layout_gravity="bottom|start"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{cuisinesRowModel.txtRestaurantsCounter}"
                    style="@style/txtDMSansmedium12" tools:text="@string/lbl_84_restaurants"/>
            </FrameLayout>
        </LinearLayout>
        <ImageView android:id="@+id/imageRectangle117"
            android:layout_height="@dimen/_17pxh"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_28pxh"
            android:layout_marginStart="@dimen/_192pxh"
            android:layout_marginTop="@dimen/_19pxv"
            android:layout_width="@dimen/_17pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_rectangle117"
            tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle117"/>
    </LinearLayout>
</layout>
