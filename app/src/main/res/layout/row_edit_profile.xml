<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="editProfileRowModel" type="com.khushalsapplication.app.modules.editprofile.data.model.EditProfileRowModel"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearColumnphonenumber"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_8">
        <TextView android:ellipsize="end"
            android:id="@+id/txtPhoneNumber"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_marginStart="@dimen/_7pxh"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{editProfileRowModel.txtPhoneNumber}"
            style="@style/txtDMSansmedium16" tools:text="@string/lbl_phone_number"/>
        <LinearLayout android:id="@+id/linearRowadd"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_6pxv"
            android:layout_marginEnd="@dimen/_11pxh"
            android:layout_marginStart="@dimen/_8pxh"
            android:layout_marginTop="@dimen/_3pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <LinearLayout android:id="@+id/linearRowrectangle248"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_6pxv"
                android:layout_weight="0.61" android:layout_width="0dp" android:orientation="horizontal">
                <ImageView android:id="@+id/imageRectangle248"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_marginBottom="@dimen/_6pxv"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_28pxh"
                    android:scaleType="centerCrop" circular="@{true}"
                    imageRes="@{@drawable/img_rectangle248}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle248"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtNinetyOne"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_weight="0.53"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{editProfileRowModel.txtNinetyOne}"
                    style="@style/txtDMSansmedium21" tools:text="@string/lbl_91"/>
            </LinearLayout>
            <TextView android:ellipsize="end" android:id="@+id/txtAdd"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_2pxv"
                android:layout_marginStart="@dimen/_289pxh"
                android:layout_weight="0.39" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{editProfileRowModel.txtAdd}"
                style="@style/txtDMSansmedium24" tools:text="@string/lbl_add"/>
        </LinearLayout>
    </LinearLayout>
</layout>
