<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="spainRowModel" type="com.khushalsapplication.app.modules.spain.data.model.SpainRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearRowrectangleeightyone"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="horizontal" style="@style/groupStylewhite_2">
        <ImageView android:id="@+id/imageRectangleEightyOne"
            android:layout_height="@dimen/_93pxh"
            android:layout_marginBottom="@dimen/_14pxv"
            android:layout_marginStart="@dimen/_29pxh"
            android:layout_marginTop="@dimen/_13pxv"
            android:layout_width="@dimen/_104pxh"
            android:scaleType="fitXY" cornerRadius="@{@dimen/_10pxh}"
            imageRes="@{@drawable/img_rectangle81_4}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle81_4"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumnnatillareceta"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_25pxv"
            android:layout_marginEnd="@dimen/_25pxh"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginTop="@dimen/_18pxv"
            android:layout_weight="0.7" android:layout_width="0dp" android:orientation="vertical">
            <LinearLayout android:id="@+id/linearRownatillareceta"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtNatillaReceta"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:layout_weight="0.75"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{spainRowModel.txtNatillaReceta}"
                    style="@style/txtDMSansbold20" tools:text="@string/lbl_natilla_receta"/>
                <LinearLayout
                    android:id="@+id/linearRowrectangleeightytwo"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_69pxh"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_weight="0.25"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageRectangleEightyTwo"
                        android:layout_height="@dimen/_16pxh"
                        android:layout_marginBottom="@dimen/_5pxv"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:layout_width="@dimen/_16pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_rectangle78"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle78"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtFortyNine"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_weight="0.63"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{spainRowModel.txtFortyNine}"
                        style="@style/txtDMSansbold18_2" tools:text="@string/lbl_4_9"/>
                </LinearLayout>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtDulcedemoras"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{spainRowModel.txtDulcedemoras}"
                style="@style/txtDMSansbold14_1" tools:text="@string/lbl_dulce_de_moras"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtTimeOne"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_9pxv"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{spainRowModel.txtTimeOne}"
                style="@style/txtDMSansbold11" tools:text="@string/msg_order_from_200r"/>
        </LinearLayout>
    </LinearLayout>
</layout>
