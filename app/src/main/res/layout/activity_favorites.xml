<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="favoritesVM" type="com.khushalsapplication.app.modules.favorites.data.viewmodel.FavoritesVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearColumnrectangleeightyseven"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/_22pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <androidx.core.widget.NestedScrollView
            android:clipToPadding="false" android:fillViewport="true"
            android:gravity="center_horizontal"
            android:id="@+id/scrollViewScrollview"
            android:layout_height="0dp"
            android:layout_marginStart="@dimen/_1pxh"
            android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:id="@+id/linearFavorites"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <ImageView android:id="@+id/imageRectangleEightySeven"
                    android:layout_gravity="end"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_marginEnd="@dimen/_34pxh"
                    android:layout_marginStart="@dimen/_34pxh"
                    android:layout_width="@dimen/_26pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_rectangle43"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle43"/>
                <LinearLayout android:id="@+id/linearColumnfavorites"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_17pxv"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtFavorites"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_26pxh"
                        android:layout_marginStart="@dimen/_26pxh"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{favoritesVM.favoritesModel.txtFavorites}"
                        style="@style/txtDMSansbold37" tools:text="@string/lbl_favorites"/>
                    <androidx.recyclerview.widget.RecyclerView
                        android:gravity="start"
                        android:id="@+id/recyclerFavorites"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_width="match_parent"
                        android:nestedScrollingEnabled="false"
                        android:orientation="vertical"
                        app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                        itemSpace="@{@dimen/_9pxh}" tools:itemCount="2" tools:listitem="@layout/row_favorites"/>
                    <LinearLayout
                        android:id="@+id/linearRowrectangle304"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <FrameLayout
                            android:id="@+id/frameStackrectangle304"
                            android:layout_height="@dimen/_203pxv" android:layout_width="@dimen/_215pxh">
                            <View android:background="@color/white_A700"
                                android:id="@+id/viewRectangle304"
                                android:layout_gravity="top|start"
                                android:layout_height="@dimen/_151pxv"
                                android:layout_marginBottom="@dimen/_10pxv" android:layout_width="@dimen/_215pxh"/>
                            <ImageView
                                android:id="@+id/imageRectangleNinety"
                                android:layout_gravity="end"
                                android:layout_height="@dimen/_197pxh"
                                android:layout_marginEnd="@dimen/_5pxh"
                                android:layout_marginStart="@dimen/_10pxh"
                                android:layout_marginTop="@dimen/_6pxv"
                                android:layout_width="@dimen/_192pxh"
                                android:scaleType="fitXY"
                                cornerRadius="@{@dimen/_15pxh}"
                                imageRes="@{@drawable/img_rectangle90}"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle90"/>
                        </FrameLayout>
                        <FrameLayout
                            android:id="@+id/frameStackrectangle305"
                            android:layout_height="@dimen/_202pxv"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginStart="@dimen/_5pxh" android:layout_width="@dimen/_209pxh">
                            <View android:background="@color/white_A700"
                                android:id="@+id/viewRectangle305"
                                android:layout_gravity="top|start"
                                android:layout_height="@dimen/_174pxv"
                                android:layout_marginBottom="@dimen/_10pxv" android:layout_width="@dimen/_209pxh"/>
                            <ImageView
                                android:id="@+id/imageRectangleNinetyOne"
                                android:layout_gravity="center"
                                android:layout_height="@dimen/_197pxh"
                                android:layout_marginEnd="@dimen/_6pxh"
                                android:layout_marginStart="@dimen/_11pxh"
                                android:layout_marginTop="@dimen/_5pxv"
                                android:layout_width="@dimen/_192pxh"
                                android:scaleType="fitXY"
                                cornerRadius="@{@dimen/_15pxh}"
                                imageRes="@{@drawable/img_rectangle91}"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle91"/>
                        </FrameLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </androidx.core.widget.NestedScrollView>
        <FrameLayout android:id="@+id/frameBottombar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" style="@style/groupStyleblack_900">
            <LinearLayout
                android:id="@+id/linearRowrectanglethirtyeight"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_43pxv"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageRectangleThirtyEight"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_31pxh"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_marginStart="@dimen/_37pxh"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_width="@dimen/_31pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_rectangle38"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle38"/>
                <ImageView android:id="@+id/imageRectangleThirtyNine"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_36pxh"
                    android:layout_marginStart="@dimen/_55pxh"
                    android:layout_width="@dimen/_28pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_rectangle39"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle39"/>
                <ImageView android:id="@+id/imageRectangleForty"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_29pxh"
                    android:layout_marginBottom="@dimen/_3pxv"
                    android:layout_marginStart="@dimen/_55pxh"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_width="@dimen/_31pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_rectangle40_29x31"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle40_29x31"/>
                <ImageView android:id="@+id/imageRectangleFortyOne"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_29pxh"
                    android:layout_marginBottom="@dimen/_3pxv"
                    android:layout_marginStart="@dimen/_55pxh"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_width="@dimen/_28pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_rectangle41_1"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle41_1"/>
                <ImageView android:id="@+id/imageRectangleSeventySix"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_29pxh"
                    android:layout_marginBottom="@dimen/_3pxv"
                    android:layout_marginEnd="@dimen/_24pxh"
                    android:layout_marginStart="@dimen/_55pxh"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_width="@dimen/_30pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_rectangle76"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle76"/>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
