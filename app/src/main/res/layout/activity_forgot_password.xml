<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="forgotPasswordVM" type="com.khushalsapplication.app.modules.forgotpassword.data.viewmodel.ForgotPasswordVM"/>
    </data>
    <LinearLayout android:id="@+id/linearColumnrectanglenine"
        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/_14pxv"
        android:layout_marginTop="@dimen/_5pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <androidx.appcompat.widget.Toolbar android:gravity="start"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_17pxh"
            android:layout_marginStart="@dimen/_17pxh"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowrectanglenine"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageRectangleNine"
                    android:layout_height="@dimen/_29pxh"
                    android:layout_width="@dimen/_29pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_rectangle94"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle94"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtForgotPassword"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_92pxh"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_weight="0.83"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{forgotPasswordVM.forgotPasswordModel.txtForgotPassword}"
                    style="@style/txtDMSansmedium19" tools:text="@string/lbl_forgot_password"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearForgotPassword"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_12pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700">
            <View android:background="@color/gray_600_0f"
                android:id="@+id/lineLineTwo"
                android:layout_gravity="start"
                android:layout_height="@dimen/_2pxv" android:layout_width="@dimen/_430pxh"/>
            <TextView android:gravity="center_horizontal"
                android:id="@+id/txtEnteryouremai"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_31pxh"
                android:layout_marginStart="@dimen/_31pxh"
                android:layout_marginTop="@dimen/_21pxv"
                android:layout_width="match_parent"
                android:text="@{forgotPasswordVM.forgotPasswordModel.txtEnteryouremai}"
                style="@style/txtDMSansmedium19" tools:text="@string/msg_enter_your_emai"/>
            <TextView android:ellipsize="end" android:gravity="start"
                android:id="@+id/txtGroup172"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_47pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_10pxv"
                android:paddingStart="@dimen/_17pxh"
                android:paddingTop="@dimen/_11pxv"
                android:singleLine="true"
                android:text="@{forgotPasswordVM.forgotPasswordModel.txtGroup172}"
                style="@style/txtSolid" tools:text="@string/lbl_email"/>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnSend"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_31pxh"
                android:layout_marginStart="@dimen/_31pxh"
                android:layout_marginTop="@dimen/_32pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_9pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_9pxv"
                android:singleLine="true"
                android:text="@string/lbl_send"
                android:textAllCaps="false"
                style="@style/btnSolidRounded_2" tools:text="@string/lbl_send"/>
            <View
                android:background="@drawable/rectangle_bg_black_900_radius_2_5"
                android:id="@+id/lineRectangleFive"
                android:layout_gravity="end"
                android:layout_height="@dimen/_5pxv"
                android:layout_marginEnd="@dimen/_126pxh"
                android:layout_marginStart="@dimen/_126pxh"
                android:layout_marginTop="@dimen/_582pxv" android:layout_width="@dimen/_159pxh"/>
        </LinearLayout>
    </LinearLayout>
</layout>
