<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="editProfileVM" type="com.khushalsapplication.app.modules.editprofile.data.viewmodel.EditProfileVM"/>
    </data>
    <LinearLayout android:id="@+id/linearColumnrectangleninetyfour"
        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/_45pxv"
        android:layout_marginTop="@dimen/_7pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <androidx.appcompat.widget.Toolbar android:gravity="start"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_18pxh"
            android:layout_marginStart="@dimen/_18pxh"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowrectangleninetyfour"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageRectangleNinetyFour"
                    android:layout_height="@dimen/_29pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_width="@dimen/_20pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_rectangle94_29x20"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle94_29x20"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtCompleteYourP"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_8pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_weight="0.91"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{editProfileVM.editProfileModel.txtCompleteYourP}"
                    style="@style/txtDMSansmedium20_1" tools:text="@string/msg_complete_your_p"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumnellipsefive"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_11pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700">
            <ImageView android:id="@+id/imageEllipseFive"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_117pxh"
                android:layout_marginEnd="@dimen/_20pxh"
                android:layout_marginStart="@dimen/_20pxh"
                android:layout_width="@dimen/_117pxh"
                android:scaleType="centerCrop" circular="@{true}"
                imageRes="@{@drawable/img_ellipse5}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse5"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtChangePhoto"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_20pxh"
                android:layout_marginStart="@dimen/_20pxh"
                android:layout_marginTop="@dimen/_19pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{editProfileVM.editProfileModel.txtChangePhoto}"
                style="@style/txtDMSansmedium24" tools:text="@string/lbl_change_photo"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnname"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_12pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylewhite_8">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtName"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_7pxh"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{editProfileVM.editProfileModel.txtName}"
                    style="@style/txtDMSansmedium16" tools:text="@string/lbl_name2"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPinkyPatel"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_14pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_7pxh"
                    android:layout_marginTop="@dimen/_13pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{editProfileVM.editProfileModel.txtPinkyPatel}"
                    style="@style/txtDMSansmedium18_1" tools:text="@string/lbl_pinky_patel"/>
            </LinearLayout>
            <androidx.recyclerview.widget.RecyclerView
                android:gravity="center_horizontal"
                android:id="@+id/recyclerEditProfile"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_27pxv"
                android:layout_width="match_parent"
                android:orientation="vertical"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                itemSpace="@{@dimen/_22pxh}" tools:itemCount="2" tools:listitem="@layout/row_edit_profile"/>
            <FrameLayout android:id="@+id/frameStackbirthday"
                android:layout_height="@dimen/_76pxv"
                android:layout_marginTop="@dimen/_20pxv" android:layout_width="match_parent">
                <LinearLayout android:id="@+id/linearColumnbirthday"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylewhite_8">
                    <TextView android:ellipsize="end"
                        android:gravity="start"
                        android:id="@+id/txtBirthday"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="@dimen/_36pxv"
                        android:layout_marginEnd="@dimen/_357pxh"
                        android:layout_marginStart="@dimen/_8pxh"
                        android:layout_marginTop="@dimen/_16pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{editProfileVM.editProfileModel.txtBirthday}"
                        style="@style/txtDMSansmedium16" tools:text="@string/lbl_birthday"/>
                </LinearLayout>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtDdmmyyyy"
                    android:layout_gravity="bottom|start"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_8pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{editProfileVM.editProfileModel.txtDdmmyyyy}"
                    style="@style/txtDMSansmedium18_2" tools:text="@string/lbl_dd_mm_yyyy"/>
            </FrameLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumngender"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylewhite_8">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtGender"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_8pxh"
                    android:layout_marginTop="@dimen/_28pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{editProfileVM.editProfileModel.txtGender}"
                    style="@style/txtDMSansmedium16" tools:text="@string/lbl_gender"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtChooseyourgen"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_9pxh"
                    android:layout_marginTop="@dimen/_12pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{editProfileVM.editProfileModel.txtChooseyourgen}"
                    style="@style/txtDMSansmedium18_1" tools:text="@string/msg_choose_your_gen"/>
            </LinearLayout>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnUpdateProfile"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_19pxh"
                android:layout_marginStart="@dimen/_20pxh"
                android:layout_marginTop="@dimen/_40pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_15pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_15pxv"
                android:singleLine="true"
                android:text="@string/lbl_update_profile"
                android:textAllCaps="false"
                style="@style/btnSolidRounded_6" tools:text="@string/lbl_update_profile"/>
        </LinearLayout>
    </LinearLayout>
</layout>
