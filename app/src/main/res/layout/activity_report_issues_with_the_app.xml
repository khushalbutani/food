<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="reportIssuesWithTheAppVM" type="com.khushalsapplication.app.modules.reportissueswiththeapp.data.viewmodel.ReportIssuesWithTheAppVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumnrectangleninetyfour"
        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/_40pxv"
        android:layout_marginTop="@dimen/_9pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true" android:gravity="start"
            android:id="@+id/scrollViewScrollview"
            android:layout_height="0dp" android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearReportissuesw"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <ImageView android:id="@+id/imageRectangleNinetyFour"
                    android:layout_height="@dimen/_34pxh"
                    android:layout_marginEnd="@dimen/_13pxh"
                    android:layout_marginStart="@dimen/_13pxh"
                    android:layout_width="@dimen/_34pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_rectangle94"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle94"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSendFeedback"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{reportIssuesWithTheAppVM.reportIssuesWithTheAppModel.txtSendFeedback}"
                    style="@style/txtDMSansmedium27" tools:text="@string/lbl_send_feedback"/>
                <TextView android:id="@+id/txtDescription"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="match_parent"
                    android:text="@{reportIssuesWithTheAppVM.reportIssuesWithTheAppModel.txtDescription}"
                    style="@style/txtDMSansmedium16_4" tools:text="@string/msg_tell_us_what_yo"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtGroup384"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_32pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_10pxv"
                    android:paddingStart="@dimen/_17pxh"
                    android:paddingTop="@dimen/_21pxv"
                    android:singleLine="true"
                    android:text="@{reportIssuesWithTheAppVM.reportIssuesWithTheAppModel.txtGroup384}"
                    style="@style/txtSolid_6" tools:text="@string/lbl_enter_feedback"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowgroup382"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_13pxh"
                    android:layout_marginStart="@dimen/_13pxh"
                    android:layout_marginTop="@dimen/_20pxv"
                    android:layout_width="match_parent"
                    android:orientation="horizontal" style="@style/groupStylewhite_9">
                    <ImageButton
                        android:background="@drawable/rectangle_bg_white_a700_border_gray_600_19_radius_30_5"
                        android:id="@+id/btnGroup382"
                        android:layout_height="@dimen/_61pxh"
                        android:layout_marginBottom="@dimen/_36pxv"
                        android:layout_marginStart="@dimen/_5pxh"
                        android:layout_marginTop="@dimen/_30pxv"
                        android:layout_width="@dimen/_61pxh"
                        android:paddingHorizontal="@dimen/_8pxh"
                        android:paddingVertical="@dimen/_8pxv"
                        android:scaleType="fitCenter"
                        android:src="@drawable/img_group294" tools:ignore="ContentDescription"/>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearColumnneedhelpwith"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_24pxv"
                        android:layout_marginEnd="@dimen/_84pxh"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_26pxv"
                        android:layout_weight="0.8"
                        android:layout_width="0dp" android:orientation="vertical">
                        <TextView android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:id="@+id/txtNeedhelpwith"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_9pxh"
                            android:layout_width="match_parent"
                            android:singleLine="true"
                            android:text="@{reportIssuesWithTheAppVM.reportIssuesWithTheAppModel.txtNeedhelpwith}"
                            style="@style/txtDMSansregular20_1" tools:text="@string/msg_need_help_with"/>
                        <TextView android:id="@+id/txtGetinstanthel"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_3pxv"
                            android:layout_width="match_parent"
                            android:text="@{reportIssuesWithTheAppVM.reportIssuesWithTheAppModel.txtGetinstanthel}"
                            style="@style/txtDMSansregular16_1" tools:text="@string/msg_get_instant_hel"/>
                    </LinearLayout>
                </LinearLayout>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnSubmitFeedbackOne"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_13pxh"
                    android:layout_marginStart="@dimen/_13pxh"
                    android:layout_marginTop="@dimen/_398pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_12pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_12pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_submit_feedback"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded_9" tools:text="@string/lbl_submit_feedback"/>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</layout>
