<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="brazilRowModel" type="com.khushalsapplication.app.modules.brazil.data.model.BrazilRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearRowrectangleeightyone"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="horizontal" style="@style/groupStylewhite_2">
        <ImageView android:id="@+id/imageRectangleEightyOne"
            android:layout_height="@dimen/_93pxh"
            android:layout_marginBottom="@dimen/_14pxv"
            android:layout_marginStart="@dimen/_29pxh"
            android:layout_marginTop="@dimen/_13pxv"
            android:layout_width="@dimen/_104pxh"
            android:scaleType="fitXY" cornerRadius="@{@dimen/_10pxh}"
            imageRes="@{@drawable/img_rectangle81_6}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle81_6"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumnbrigadeiro"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_25pxv"
            android:layout_marginEnd="@dimen/_25pxh"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginTop="@dimen/_20pxv"
            android:layout_weight="0.7" android:layout_width="0dp" android:orientation="vertical">
            <LinearLayout android:id="@+id/linearRowbrigadeiro"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <FrameLayout android:id="@+id/frameStackbrigadeiro"
                    android:layout_height="@dimen/_27pxv"
                    android:layout_marginBottom="@dimen/_3pxv" android:layout_width="@dimen/_99pxh">
                    <TextView android:ellipsize="end"
                        android:gravity="top|start"
                        android:id="@+id/txtBrigadeiro"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{brazilRowModel.txtBrigadeiro}"
                        style="@style/txtDMSansbold20" tools:text="@string/lbl_brigadeiro"/>
                    <TextView android:ellipsize="end"
                        android:gravity="top|start"
                        android:id="@+id/txtBrigadeiroOne"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{brazilRowModel.txtBrigadeiroOne}"
                        style="@style/txtDMSansbold20" tools:text="@string/lbl_brigadeiro"/>
                </FrameLayout>
                <LinearLayout
                    android:id="@+id/linearRowrectangleeightytwo"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_103pxh"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_weight="0.31"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageRectangleEightyTwo"
                        android:layout_height="@dimen/_16pxh"
                        android:layout_marginBottom="@dimen/_5pxv"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:layout_width="@dimen/_16pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_rectangle78"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle78"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtFortyNine"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_2pxh"
                        android:layout_weight="0.63"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{brazilRowModel.txtFortyNine}"
                        style="@style/txtDMSansbold18_2" tools:text="@string/lbl_4_9"/>
                </LinearLayout>
            </LinearLayout>
            <TextView android:ellipsize="end" android:id="@+id/txtPrice"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{brazilRowModel.txtPrice}"
                style="@style/txtDMSansbold14_1" tools:text="@string/msg_tartufini_avoc"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtTimeOne"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_9pxv"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{brazilRowModel.txtTimeOne}"
                style="@style/txtDMSansbold11" tools:text="@string/msg_order_from_200r"/>
        </LinearLayout>
    </LinearLayout>
</layout>
