<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="notificationsVM" type="com.khushalsapplication.app.modules.notifications.data.viewmodel.NotificationsVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumnnotifications"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/_22pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_18pxh"
            android:layout_marginStart="@dimen/_27pxh"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linearRownotifications"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtNotifications"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_22pxv"
                    android:layout_weight="0.89"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{notificationsVM.notificationsModel.txtNotifications}"
                    style="@style/txtDMSansbold40" tools:text="@string/lbl_notifications"/>
                <ImageView android:id="@+id/imageRectangleFortyThree"
                    android:layout_height="@dimen/_27pxh"
                    android:layout_marginBottom="@dimen/_48pxv"
                    android:layout_marginStart="@dimen/_133pxh"
                    android:layout_width="@dimen/_27pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_rectangle43"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle43"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearNotifications"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_16pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700">
            <androidx.recyclerview.widget.RecyclerView
                android:gravity="start"
                android:id="@+id/recyclerNotifications"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="vertical"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                itemSpace="@{@dimen/_24pxh}" tools:itemCount="5" tools:listitem="@layout/row_notifications"/>
        </LinearLayout>
        <FrameLayout android:gravity="start"
            android:id="@+id/frameBottombar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" style="@style/groupStyleblack_900">
            <LinearLayout
                android:id="@+id/linearRowrectanglethirtyeight"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_43pxv"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageRectangleThirtyEight"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_31pxh"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_marginStart="@dimen/_37pxh"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_width="@dimen/_31pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_rectangle38"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle38"/>
                <ImageView android:id="@+id/imageRectangleThirtyNine"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_36pxh"
                    android:layout_marginStart="@dimen/_55pxh"
                    android:layout_width="@dimen/_28pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_rectangle39"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle39"/>
                <ImageView android:id="@+id/imageRectangleForty"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_29pxh"
                    android:layout_marginBottom="@dimen/_3pxv"
                    android:layout_marginStart="@dimen/_55pxh"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_width="@dimen/_31pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_rectangle40"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle40"/>
                <ImageView android:id="@+id/imageRectangleFortyOne"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_29pxh"
                    android:layout_marginBottom="@dimen/_3pxv"
                    android:layout_marginStart="@dimen/_55pxh"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_width="@dimen/_28pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_rectangle41_29x28"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle41_29x28"/>
                <ImageView android:id="@+id/imageRectangleSeventySix"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_29pxh"
                    android:layout_marginBottom="@dimen/_3pxv"
                    android:layout_marginEnd="@dimen/_24pxh"
                    android:layout_marginStart="@dimen/_55pxh"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_width="@dimen/_30pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_rectangle76"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle76"/>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
