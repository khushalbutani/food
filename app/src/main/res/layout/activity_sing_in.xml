<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="singInVM" type="com.khushalsapplication.app.modules.singin.data.viewmodel.SingInVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumnwelcomeback"
        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/_52pxv"
        android:layout_marginEnd="@dimen/_25pxh"
        android:layout_marginStart="@dimen/_41pxh"
        android:layout_marginTop="@dimen/_55pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true" android:gravity="start"
            android:id="@+id/scrollViewScrollview"
            android:layout_height="0dp" android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:id="@+id/linearSingin"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <TextView android:gravity="start"
                    android:id="@+id/txtWelcomeback"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_21pxh"
                    android:layout_marginStart="@dimen/_21pxh"
                    android:layout_width="match_parent"
                    android:text="@{singInVM.singInModel.txtWelcomeback}"
                    style="@style/txtDMSansbold47" tools:text="@string/lbl_welcome_back"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSignintocont"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_21pxh"
                    android:layout_marginStart="@dimen/_21pxh"
                    android:layout_marginTop="@dimen/_32pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{singInVM.singInModel.txtSignintocont}"
                    style="@style/txtDMSansmedium20" tools:text="@string/msg_sign_in_to_cont"/>
                <EditText
                    android:background="@drawable/rectangle_bg_white_a701"
                    android:ellipsize="end"
                    android:hint="@string/lbl_email"
                    android:id="@+id/etGroupThirtyFour"
                    android:inputType="textEmailAddress"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_53pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_7pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_11pxh"
                    android:paddingTop="@dimen/_8pxv"
                    android:singleLine="true"
                    android:text="@={singInVM.singInModel.etGroupThirtyFourValue}"
                    android:textColorHint="@color/black_900"
                    style="@style/etSolid" tools:ignore="Autofill"/>
                <EditText
                    android:background="@drawable/rectangle_bg_white_a701"
                    android:ellipsize="end"
                    android:hint="@string/lbl_password"
                    android:id="@+id/etGroupThirtyFive"
                    android:inputType="textPassword"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_7pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_10pxh"
                    android:paddingTop="@dimen/_8pxv"
                    android:singleLine="true"
                    android:text="@={singInVM.singInModel.etGroupThirtyFiveValue}"
                    android:textColorHint="@color/black_900"
                    style="@style/etSolid" tools:ignore="Autofill"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtForgotpassword"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_3pxh"
                    android:layout_marginTop="@dimen/_23pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{singInVM.singInModel.txtForgotpassword}"
                    style="@style/txtDMSansregular16" tools:text="@string/msg_forgot_password"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnSignIn"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_14pxh"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_marginTop="@dimen/_20pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_6pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_6pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_sign_in2"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded_1" tools:text="@string/lbl_sign_in2"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowrectangleeight"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_14pxh"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_marginTop="@dimen/_19pxv"
                    android:layout_width="match_parent"
                    android:orientation="horizontal" style="@style/groupStyleblack_900cornerRadius">
                    <ImageView android:id="@+id/imageRectangleEight"
                        android:layout_height="@dimen/_24pxh"
                        android:layout_marginBottom="@dimen/_6pxv"
                        android:layout_marginStart="@dimen/_60pxh"
                        android:layout_marginTop="@dimen/_7pxv"
                        android:layout_width="@dimen/_24pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_rectangle8"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle8"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtSigninwithFa"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_6pxv"
                        android:layout_marginEnd="@dimen/_66pxh"
                        android:layout_marginStart="@dimen/_15pxh"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_weight="0.86"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{singInVM.singInModel.txtSigninwithFa}"
                        style="@style/txtDMSansmedium16_2" tools:text="@string/msg_sign_in_with_fa"/>
                </LinearLayout>
                <TextView android:ellipsize="end"
                    android:gravity="start"
                    android:id="@+id/txtConfirmation"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_62pxh"
                    android:layout_marginStart="@dimen/_62pxh"
                    android:layout_marginTop="@dimen/_267pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{singInVM.singInModel.txtConfirmation}"
                    style="@style/txtDMSansmedium16_1" tools:text="@string/msg_don_t_have_an_a"/>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</layout>
