<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="deliveryRowModel" type="com.khushalsapplication.app.modules.delivery.data.model.DeliveryRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearRowrectangle177"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="horizontal" style="@style/groupStylegray_50">
        <ImageView android:id="@+id/imageRectangle177"
            android:layout_height="@dimen/_141pxh"
            android:layout_marginBottom="@dimen/_14pxv"
            android:layout_marginStart="@dimen/_17pxh"
            android:layout_width="@dimen/_140pxh"
            android:scaleType="fitXY" cornerRadius="@{@dimen/_7pxh}"
            imageRes="@{@drawable/img_rectangle177}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle177"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumnbigdouble"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_30pxv"
            android:layout_marginEnd="@dimen/_64pxh"
            android:layout_marginStart="@dimen/_19pxh"
            android:layout_marginTop="@dimen/_4pxv"
            android:layout_weight="0.57" android:layout_width="0dp" android:orientation="vertical">
            <TextView android:ellipsize="end"
                android:id="@+id/txtBigDouble"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{deliveryRowModel.txtBigDouble}"
                style="@style/txtDMSansbold20" tools:text="@string/lbl_big_double"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRow1500rs"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_7pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txt1500rs"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{deliveryRowModel.txt1500rs}"
                    style="@style/txtDMSansbold16" tools:text="@string/lbl_1500rs"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txt250gm"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_28pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{deliveryRowModel.txt250gm}"
                    style="@style/txtDMSansbold16" tools:text="@string/lbl_250gm"/>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtBurgerGooby"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_5pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{deliveryRowModel.txtBurgerGooby}"
                style="@style/txtDMSansmedium13_1" tools:text="@string/lbl_burger_gooby"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowcalendar"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageCalendar"
                    android:layout_height="@dimen/_16pxh"
                    android:layout_marginBottom="@dimen/_7pxv"
                    android:layout_width="@dimen/_16pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_calendar"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_calendar"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTime"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_9pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.91"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{deliveryRowModel.txtTime}"
                    style="@style/txtDMSansregular17_1" tools:text="@string/msg_9_apr_2023_3_2"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
