<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="ordersVM" type="com.khushalsapplication.app.modules.orders.data.viewmodel.OrdersVM"/>
    </data>
    <LinearLayout android:id="@+id/linearColumnfilter"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/_18pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <androidx.appcompat.widget.Toolbar
            android:gravity="center_horizontal"
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_23pxh"
            android:layout_marginStart="@dimen/_23pxh"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linearRowfilter"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtFilter"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_weight="0.64"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{ordersVM.ordersModel.txtFilter}"
                    style="@style/txtDMSansmedium18_1" tools:text="@string/lbl_filter"/>
                <ImageView android:id="@+id/imageRectangle134"
                    android:layout_height="@dimen/_24pxh"
                    android:layout_marginStart="@dimen/_301pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_width="@dimen/_24pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_rectangle134"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle134"/>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:id="@+id/linearColumnorders"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_14pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700">
            <TextView android:ellipsize="end"
                android:id="@+id/txtOrders"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_23pxh"
                android:layout_marginStart="@dimen/_23pxh"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{ordersVM.ordersModel.txtOrders}"
                style="@style/txtDMSansbold36" tools:text="@string/lbl_orders"/>
            <androidx.recyclerview.widget.RecyclerView
                android:gravity="start" android:id="@+id/recyclerOrders"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_8pxv"
                android:layout_width="match_parent"
                android:orientation="vertical"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                itemSpace="@{@dimen/_10pxh}" tools:itemCount="4" tools:listitem="@layout/row_orders"/>
        </LinearLayout>
        <FrameLayout android:gravity="start"
            android:id="@+id/frameBottombar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" style="@style/groupStyleblack_900">
            <LinearLayout
                android:id="@+id/linearRowrectanglethirtyeight"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_43pxv"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageRectangleThirtyEight"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_31pxh"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_marginStart="@dimen/_37pxh"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_width="@dimen/_31pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_rectangle38"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle38"/>
                <ImageView android:id="@+id/imageRectangleThirtyNine"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_36pxh"
                    android:layout_marginStart="@dimen/_55pxh"
                    android:layout_width="@dimen/_28pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_rectangle39_36x28"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle39_36x28"/>
                <ImageView android:id="@+id/imageRectangle143"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_29pxh"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:layout_marginStart="@dimen/_55pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_width="@dimen/_31pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_rectangle40"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle40"/>
                <ImageView android:id="@+id/imageRectangleFortyOne"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_29pxh"
                    android:layout_marginBottom="@dimen/_3pxv"
                    android:layout_marginStart="@dimen/_55pxh"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_width="@dimen/_28pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_rectangle41_1"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle41_1"/>
                <ImageView android:id="@+id/imageRectangleSeventySix"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_29pxh"
                    android:layout_marginBottom="@dimen/_3pxv"
                    android:layout_marginEnd="@dimen/_24pxh"
                    android:layout_marginStart="@dimen/_55pxh"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_width="@dimen/_30pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_rectangle76"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle76"/>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
