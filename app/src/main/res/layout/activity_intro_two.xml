<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="introTwoVM" type="com.khushalsapplication.app.modules.introtwo.data.viewmodel.IntroTwoVM"/>
    </data>
    <FrameLayout android:id="@+id/frameIntroTwo"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <ImageView android:id="@+id/imageImageFour"
            android:layout_gravity="start"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:scaleType="fitXY" imageRes="@{@drawable/img_image4}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_image4"/>
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true"
            android:id="@+id/scrollViewScrollview"
            android:layout_gravity="start" android:layout_height="0dp"
            android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:id="@+id/linearColumnskip"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStyleblack_900_82">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSkip"
                    android:layout_gravity="end"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_29pxh"
                    android:layout_marginStart="@dimen/_29pxh"
                    android:layout_marginTop="@dimen/_21pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{introTwoVM.introTwoModel.txtSkip}"
                    style="@style/txtDMSansbold18" tools:text="@string/lbl_skip"/>
                <TextView android:gravity="start"
                    android:id="@+id/txtSearchforplac"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_29pxh"
                    android:layout_marginStart="@dimen/_29pxh"
                    android:layout_marginTop="@dimen/_148pxv"
                    android:layout_width="match_parent"
                    android:text="@{introTwoVM.introTwoModel.txtSearchforplac}"
                    style="@style/txtDMSansbold52" tools:text="@string/msg_search_for_pla"/>
                <TextView android:gravity="start"
                    android:id="@+id/txtDescription"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_29pxh"
                    android:layout_marginStart="@dimen/_29pxh"
                    android:layout_marginTop="@dimen/_14pxv"
                    android:layout_width="match_parent"
                    android:text="@{introTwoVM.introTwoModel.txtDescription}"
                    style="@style/txtDMSansbold18" tools:text="@string/msg_decide_where_to"/>

                <com.asksira.loopingviewpager.indicator.CustomShapePagerIndicator
                    android:id="@+id/indicatorGroup449"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_63pxv"
                    android:layout_marginEnd="@dimen/_29pxh"
                    android:layout_marginStart="@dimen/_29pxh"
                    android:layout_marginTop="@dimen/_390pxv"
                    android:layout_width="wrap_content"
                    app:drawable_normal="@drawable/indicator_unselected_bg_gray_600_aa_radius_8"
                    app:drawable_selected="@drawable/indicator_selected_bg_white_a700_radius_8"
                    app:indicator_size="@dimen/_7pxh" app:indicator_spacing="@dimen/_10pxh"/>
            </LinearLayout>
        </ScrollView>
    </FrameLayout>
</layout>
